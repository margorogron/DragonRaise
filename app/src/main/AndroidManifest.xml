<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dragonraise">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ikonka"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ikonka"
        android:supportsRtl="true"
        android:theme="@style/Theme.DragonRaise">
        <activity android:name=".Puzzle"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Game" />
        <activity android:name=".Rules" />
        <activity android:name=".User" />
    </application>

</manifest>